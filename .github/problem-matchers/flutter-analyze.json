{
  "problemMatcher": [
    {
      "owner": "flutter-analyze",
      "pattern": [
        {
          "regexp": "^\\s*(\\w*) • (.*?) • (.*):([0-9]+):([0-9]+) • .*$",
          "severity": 1,
          "message": 2,
          "file": 3,
          "line": 4,
          "column": 5
        }
      ]
    }
  ]
}
